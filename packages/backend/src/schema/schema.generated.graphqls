type Game {
  id: ID!
  name: String!
}

type Mutation {
  loginWithPassword(email: String!, password: String!): User!
  loginWithRefreshToken: User!
  registerWithPassword(email: String!, name: String!, password: String!): User!
}

type Planet {
  id: ID!
  name: String!
  position: Vector!
}

type Query {
  games: [Game!]!
  me: User
  planets: [Planet!]!
}

type User {
  email: String!
  id: ID!
  name: String!
}

scalar Vector